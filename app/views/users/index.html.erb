<div class="Project">

	<div id="Header">

		<div id="Logo">
			<img src="/logo2.jpg", alt="Can't load image" />
		</div>

		<div id="Menu">
			<ul>
				<li>
					Strona glowna
				</li>
				<li>
					Nowosci
				</li>
				<li>
					Produkty
				</li>
				<li>
					Informacje
				</li>
			</ul>
		</div>

	</div>

	<div id="Content">

		<div id="LeftSide">

			Kategorie:
		</div>
		<div id="Parent">
			<div id="FrontOT">
			</br>
				<div class="NewUser_Form">
				
						<h1 class="Headers">Listing users</h1>
				
						<table>
						  <thead >
							<tr  id="Row1">
							  <th>Name</th>
							  <th>Email</th>
							  <th>Password</th>
							  <th>Group</th>
							  <th colspan="3">Actions</th>
							</tr>
						  </thead>
							
						  <tbody>
							<% @users.each do |user| %>
							  <tr>
								<td><%= user.name %></td>
								<td><%= user.email %></td>
								<td><%= user.password %></td>
								<td><%= user.group.name %></td>
								<td><%= link_to 'Show', user %></td>
								<td><%= link_to 'Edit', edit_user_path(user) %></td>
								<td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
							  </tr>
							<% end %>
						  </tbody>
						</table>
						</br>
						<div id="create_new_user">
							<%= link_to 'New User', new_user_path %>
						</div>
				</div>
			</div>
		</div>
	</div>

	<div id="Footer">
			Infolinia: 987 654 321 |
			e-mail: e-sklep@info.pl |
			pon-pt: 10.00-20.00
	</div>
</div>











